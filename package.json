{
  "name":            "@exygen/archimate",
  "version":         "1.0.0",
  "private":         true,
  "scripts":         {
	"prebuild": "npm link @exygen/archimate-model",
	"build":        "npm run build:server && npm run build:front && npm run build:shared",
	"build:server": "cd archimate-server && npm run build",
	"build:front":  "cd archimate-front && npm run build",
	"build:shared": "cd archimate-model && npm run build"
  },
  "workspaces":      [
	"packages/*"
  ],
  "dependencies":    {
	"body-parser": "^1.20.2",
	"core-js":     "^3.8.3",
	"cors":        "^2.8.5",
	"express":     "^4.18.2",
	"mysql":       "^2.18.1",
	"uuid":        "^9.0.0"
  },
  "devDependencies": {
	"@babel/core":       "^7.21.8",
	"@babel/preset-env": "^7.21.5",
	"babel-loader":      "^9.1.2",
	"path":              "^0.12.7",
	"webpack-cli":       "^5.1.1",
	"eslint":            "^8.22.0",
	"babel":             "^6.23.0",
	"babel-cli":         "^6.26.0",
	"jsdom":             "^20.0.1"
  }
}
